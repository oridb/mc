use std
use "testmatch"

const main = {
	testmatch("A(.*)", "Abc", `std.Some ["bc"][:])
	testmatch("A(.*)e", "Abcde", `std.Some ["bcd"][:])
	testmatch("(a|b)+", "abab", `std.Some ["b"][:])
	testmatch("A(b(.*)d)e", "Abcde", `std.Some [
		"bcd",
		"c"
	][:])
	testmatch("(a?)(a*)(a?)", "aaaa", `std.Some [
		"a",
		"aaa",
		""
	][:])
}
