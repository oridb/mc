		[.name="keccak512smoke", .fn={ctx
			var ret

			ret = crypto.keccak512("Keccak-512 Test Hash")
			testr.check(ctx, \
				std.eq(\
					ret[:],  \
					"\x96\xEE\x47\x18\xDC\xBA\x3C\x74" \
					"\x61\x9B\xA1\xFA\x7F\x57\xDF\xE7" \
